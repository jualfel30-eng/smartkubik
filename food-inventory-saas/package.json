{
  "name": "food-inventory-saas",
  "version": "1.0.0",
  "description": "SaaS de inventario + Ã³rdenes + CRM para comercio alimentario en Venezuela",
  "author": "Food Inventory Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:security": "jest --testPathPattern='(sanitization|rate-limiting|ownership-validation|csp-headers)' --coverage --verbose",
    "test:security:unit": "jest test/unit/sanitization.spec.ts --coverage",
    "test:security:integration": "jest test/integration/rate-limiting.spec.ts --verbose",
    "test:security:e2e": "jest --config ./test/jest-e2e.json --testPathPattern='(ownership-validation|csp-headers)' --verbose",
    "test:all": "npm run test && npm run test:e2e",
    "db:seed": "ts-node -r dotenv/config scripts/seed-database.ts",
    "db:migrate:weight-selling": "ts-node -r dotenv/config scripts/migrate-products-for-weight-selling.ts",
    "db:migrate:tenant-ids": "ts-node -r dotenv/config scripts/migrate-tenant-ids-to-string.ts",
    "db:migrate:vertical": "ts-node -r dotenv/config scripts/migrate-tenants-vertical.ts",
    "db:migrate:hospitality-hand-off": "ts-node -r dotenv/config scripts/migrate-hospitality-hand-off.ts",
    "seed:plans": "ts-node -r dotenv/config scripts/seed-subscription-plans.ts",
    "seed:restaurant": "ts-node -r dotenv/config scripts/seed-restaurant.ts",
    "db:reset": "ts-node scripts/reset-database.ts",
    "debug:data": "ts-node -r dotenv/config -r tsconfig-paths/register scripts/debug-data.ts",
    "seed:super-admin": "ts-node -r dotenv/config scripts/seed-super-admin.ts",
    "clear-tenant": "tsx scripts/clear-tenant-data.ts",
    "verify-tenant": "tsx scripts/verify-tenant-data.ts",
    "fix-user-tenant": "tsx scripts/fix-user-tenant.ts",
    "load-test:hospitality-notifications": "ts-node -r dotenv/config scripts/hospitality-notification-load-test.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/throttler": "^5.2.0",
    "@nestjs/websockets": "^10.4.20",
    "@langchain/core": "0.3.78",
    "@langchain/openai": "0.6.14",
    "@langchain/pinecone": "0.2.0",
    "@pinecone-database/pinecone": "^5.0.2",
    "nest-winston": "^1.9.3",
    "@nestjs/bullmq": "^10.2.1",
    "@types/nodemailer": "^7.0.1",
    "@types/passport-google-oauth20": "^2.0.16",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "bullmq": "^5.8.3",
    "langchain": "^0.3.36",
    "luxon": "^3.5.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.6.3",
    "nodemailer": "^7.0.6",
    "openai": "^6.3.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^2.2.16",
    "winston": "^3.13.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/sanitize-html": "^2.16.0",
    "@types/supertest": "^2.0.12",
    "@types/xlsx": "^0.0.35",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.1.3",
    "webpack": "^5.101.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "mjs"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest",
      "^.+\\.mjs$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!pdfjs-dist/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/**/*.spec.ts",
      "!src/**/*.e2e-spec.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^pdf-parse$": "<rootDir>/test/__mocks__/fileMock.js"
    }
  }
}
